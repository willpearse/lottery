	M1 = {ArticleType: research-article / Full publication date: Sep., 2004 / Copyright {\copyright}2004 American Statistical Association},
	Number = {467},
	Pages = {673--686},
	Publisher = {American Statistical Association},
	Title = {Stable and Efficient Multiple Smoothing Parameter Estimation for Generalized Additive Models},
	Ty = {JOUR},
