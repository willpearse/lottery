	Issn = {0006-341X},
	Journal = {Biometrics},
	Keywords = {Analysis of Variance,Animals,Biometry,Birds,Birds: classification,Body Weight,Female,Fishes,Fishes: classification,Fishes: growth \& development,Life Expectancy,Mammals,Mammals: anatomy \& histology,Mammals: classification,Models,Phylogeny,Species Specificity,Statistical,phylogenetic scale,phylogenetic signal,phylogenetic structure,statistics},
	Mendeley-Tags = {phylogenetic scale,phylogenetic signal,phylogenetic structure,statistics},
