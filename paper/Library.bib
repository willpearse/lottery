	Pages = {e9451--},
	Publisher = {Public Library of Science},
	Title = {Interspecific Hybridization as a Tool to Understand Vocal Divergence: The Example of Crowing in Quail (Genus <italic>Coturnix</italic>)},
	Ty = {JOUR},
	Volume = {5},
	Year = {2010},
