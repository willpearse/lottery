	Issn = {0012-9658},
	Journal = {Ecology},
	Keywords = {biological invasions,community phylogenetics,community structure,formicidae},
	Month = {October},
	Number = {10},
	Pages = {2664--9},
	Pmid = {19886475},
	Title = {{Invasive ants alter the phylogenetic structure of ant communities.}},
	Volume = {90},
	Year = {2009},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/19886475}}

@article{Letcher2009,
	Abstract = {The phylogenetic structure of ecological communities can shed light on assembly processes, but the focus of phylogenetic structure research thus far has been on mature ecosystems. Here, I present the first inves- tigation of phylogenetic community structure during succession. In a replicated chronosequence of 30 sites in northeastern Costa Rica, I found strong phylogenetic overdispersion at multiple scales: species present at local sites were a non-random assemblage, more distantly related than chance would predict. Phylogenetic overdispersion was evident when comparing the species present at each site with the regional species pool, the species pool found in each age category to the regional pool or the species present at each site to the pool of species found in sites of that age category. Comparing stem size classes within each age category, I found that during early succession, phylogenetic overdispersion is strongest in small stems. Overdispersion strengthens and spreads into larger size classes as succession proceeds, corroborating an existing model of forest succession. This study is the first evidence that succession leaves a distinct signature in the phylogenetic structure of communities.},
	Annote = {NRI and NTI values show that tropical stands remain overdispersed, but bigger stems get mroe and more overdispersed over time. They argue this is a signal of dispersion, and it's pretty cool.},
	Author = {Letcher, S.G.},
	Date-Added = {2010-01-29 11:25:41 +0000},
	Date-Modified = {2010-01-29 11:26:49 +0000},
	File = {:Users/will/Documents/Sorted Papers/Letcher - 2009 - Phylogenetic structure of angiosperm communities during tropical forest succession.pdf:pdf},
	Journal = {Proceedings of the Royal Society B},
	Keywords = {chronosequence,community assembly,community phylogenetics,ecology,phylogenetic structure,succession,tropical forest},
	Mendeley-Tags = {community assembly,community phylogenetics,ecology,succession},
	Number = {November},
	Publisher = {The Royal Society},
	Title = {{Phylogenetic structure of angiosperm communities during tropical forest succession}},
	Year = {2009},
	Bdsk-Url-1 = {http://rspb.royalsocietypublishing.org/cgi/content/abstract/rspb.2009.0865v1},
	Bdsk-Url-2 = {http://dx.doi.org/10.1098/rspb.2009.0865}}

