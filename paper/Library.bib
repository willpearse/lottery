@article{Jones2011,
	Abstract = { <sec> <title>Background</title> <p>Plants interact with each other, nutrients, and microbial communities in soils through extensive root networks. Understanding these below ground interactions has been difficult in natural systems, particularly those with high plant species diversity where morphological identification of fine roots is difficult. We combine DNA-based root identification with a DNA barcode database and above ground stem locations in a floristically diverse lowland tropical wet forest on Barro Colorado Island, Panama, where all trees and lianas &gt;1 cm diameter have been mapped to investigate richness patterns below ground and model rooting distributions.</p> </sec> <sec> <title>Methodology/Principal Findings</title> <p>DNA barcode loci, particularly the cpDNA locus <italic>trnH-psba</italic>, can be used to identify fine and small coarse roots to species. We recovered 33 species of roots from 117 fragments sequenced from 12 soil cores. Despite limited sampling, we recovered a high proportion of the known species in the focal hectare, representing approximately 14% of the measured woody plant richness. This high value is emphasized by the fact that we would need to sample on average 13 m<sup>2</sup> at the seedling layer and 45 m<sup>2</sup> for woody plants &gt;1 cm diameter to obtain the same number of species above ground. Results from inverse models parameterized with the locations and sizes of adults and the species identifications of roots and sampling locations indicates a high potential for distal underground interactions among plants.</p> </sec> <sec> <title>Conclusions</title> <p>DNA barcoding techniques coupled with modeling approaches should be broadly applicable to studying root distributions in any mapped vegetation plot. We discuss the implications of our results and outline how second-generation sequencing technology and environmental sampling can be combined to increase our understanding of how root distributions influence the potential for plant interactions in natural ecosystems.</p> </sec> },
	Author = {Jones, F. Andrew and Erickson, David L. and Bernal, Moises A. and Bermingham, Eldredge and Kress, W. John and Herre, Edward Allen and Muller-Landau, Helene C. and Turner, Benjamin L.},
	Date = {2011/09/19},
	Date-Added = {2011-10-10 13:10:35 +0000},
	Date-Modified = {2011-10-10 13:10:35 +0000},
	Journal = {PLoS ONE},
	Journal1 = {PLoS ONE},
	M3 = {doi:10.1371/journal.pone.0024506},
	Month = {09},
	Number = {9},
	Publisher = {Public Library of Science},
	Sp = {e24506 EP -},
	Title = {The Roots of Diversity: Below Ground Species Richness and Rooting Distributions in a Tropical Forest Revealed by DNA Barcodes and Inverse Modeling},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pone.0024506},
	Volume = {6},
	Year = {2011},
	Bdsk-Url-1 = {http://dx.doi.org/10.1371%2Fjournal.pone.0024506}}

