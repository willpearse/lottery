	Title = {Age and rate of diversification of the Hawaiian silversword alliance (Compositae)},
	Ty = {JOUR},
	Volume = {95},
	Year = {1998},
	Bdsk-Url-1 = {http://www.pnas.org/content/95/16/9402.abstract%20N2%20-%20Comparisons%20between%20insular%20and%20continental%20radiations%20have%20been%20hindered%20by%20a%20lack%20of%20reliable%20estimates%20of%20absolute%20diversification%20rates%20in%20island%20lineages.%20We%20took%20advantage%20of%20rate-constant%20rDNA%20sequence%20evolution%20and%20an%20%E2%80%9Cexternal%E2%80%9D%20calibration%20using%20paleoclimatic%20and%20fossil%20data%20to%20determine%20the%20maximum%20age%20and%20minimum%20diversification%20rate%20of%20the%20Hawaiian%20silversword%20alliance%20(Compositae),%20a%20textbook%20example%20of%20insular%20adaptive%20radiation%20in%20plants.%20Our%20maximum-age%20estimate%20of%205.2%20%C2%B1%200.8%20million%20years%20ago%20for%20the%20most%20recent%20common%20ancestor%20of%20the%20silversword%20alliance%20is%20much%20younger%20than%20ages%20calculated%20by%20other%20means%20for%20the%20Hawaiian%20drosophilids,%20lobelioids,%20and%20honeycreepers%20and%20falls%20approximately%20within%20the%20history%20of%20the%20modern%20high%20islands%20(%E2%89%A45.1%20%C2%B1%200.2%20million%20years%20ago).%20By%20using%20a%20statistically%20efficient%20estimator%20that%20reduces%20error%20variance%20by%20incorporating%20clock-based%20estimates%20of%20divergence%20times,%20a%20minimum%20diversification%20rate%20for%20the%20silversword%20alliance%20was%20estimated%20to%20be%200.56%20%C2%B1%200.17%20species%20per%20million%20years.%20This%20exceeds%20average%20rates%20of%20more%20ancient%20continental%20radiations%20and%20is%20comparable%20to%20peak%20rates%20in%20taxa%20with%20sufficiently%20rich%20fossil%20records%20that%20changes%20in%20diversification%20rate%20can%20be%20reconstructed.}}

@article{Seehausen2006,
	Annote = {10.1098/rspb.2006.3539},
	Author = {Seehausen, Ole},
	Date = {2006/08/22},
	Date-Added = {2011-06-07 13:57:40 +0200},
