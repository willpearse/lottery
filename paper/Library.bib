	Issn = {1476-4687},
	Journal = {Nature},
	Keywords = {Animals,Caribbean Region,Cuba,DNA, Mitochondrial,DNA, Mitochondrial: genetics,Ecology,Environment,Evolution,Lizards,Lizards: classification,Lizards: genetics,Lizards: physiology,Phylogeny,Trees,adaptive radiation,evolution,niche,niche conservatism},
	Mendeley-Tags = {adaptive radiation,evolution,niche,niche conservatism},
