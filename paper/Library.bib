	Pages = {--},
	Title = {The Lucretian swerve: The biological basis of human behavior and the criminal justice system},
	Ty = {JOUR},
	Year = {2010},
	Bdsk-Url-1 = {http://www.pnas.org/content/early/2010/02/04/0915161107.abstract}}

@article{Edwards2010,
	Abstract = {Grasslands cover more than 20{\%} of the Earth's terrestrial surface, and their rise to dominance is one of the most dramatic events of biome evolution in Earth history. Grasses possess two main photosynthetic pathways: the C pathway that is typical of most plants and a specialized C pathway that minimizes photorespiration and thus increases photosynthetic performance in high-temperature and/or low-CO environments. C grasses dominate tropical and subtropical grasslands and savannas, and C grasses dominate the world's cooler temperate grassland regions. This striking pattern has been attributed to C physiology, with the implication that the evolution of the pathway enabled C grasses to persist in warmer climates than their C relatives. We combined geospatial and molecular sequence data from two public archives to produce a 1,230-taxon phylogeny of the grasses with accompanying climate data for all species, extracted from more than 1.1 million herbarium specimens. Here we show that grasses are ancestrally a warm-adapted clade and that C evolution was not correlated with shifts between temperate and tropical biomes. Instead, 18 of 20 inferred C origins were correlated with marked reductions in mean annual precipitation. These changes are consistent with a shift out of tropical forest environments and into tropical woodland/savanna systems. We conclude that C evolution in grasses coincided largely with migration out of the understory and into open-canopy environments. Furthermore, we argue that the evolution of cold tolerance in certain C lineages is an overlooked innovation that has profoundly influenced the patterning of grassland communities across the globe. },
	Annote = {10.1073/pnas.0909672107},
	Author = {Edwards, Erika J. and Smith, Stephen A.},
	Date = {2010/02/08},
	Date-Added = {2010-02-09 09:59:41 +0000},
	Date-Modified = {2011-08-15 10:15:31 +0000},
	Journal = {Proceedings of the National Academy of Sciences},
	Journal1 = {Proceedings of the National Academy of Sciences},
	Month = {02},
	Pages = {--},
	Title = {Phylogenetic analyses reveal the shady history of C4 grasses},
	Ty = {JOUR},
	Year = {2010},
	Bdsk-Url-1 = {http://www.pnas.org/content/early/2010/02/02/0909672107.abstract}}

