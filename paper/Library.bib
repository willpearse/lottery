	Issn = {00220477},
	Journal = {Journal of Ecology},
	Keywords = {community phylogenetics,facilitation},
	Mendeley-Tags = {community phylogenetics,facilitation},
	Number = {6},
	Pages = {1171--1180},
	Title = {{Phylogenetic signatures of facilitation and competition in successional communities}},
	Volume = {97},
	Year = {2009},
