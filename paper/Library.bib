@article{Silvertown2006,
	Abstract = {R. H. Whittaker's idea that plant diversity can be divided into a hierarchy of spatial components from \^{I}$\pm$ at the within-habitat scale through \^{I}² for the turnover of species between habitats to \^{I}³ along regional gradients implies the underlying existence of \^{I}$\pm$, \^{I}², and \^{I}³ niches. We explore the hypothesis that the evolution of \^{I}$\pm$, \^{I}², and \^{I}³ niches is also hierarchical, with traits that define the \^{I}$\pm$ niche being labile, while those defining \^{I}² and \^{I}³ niches are conservative. At the \^{I}$\pm$ level we find support for the hypothesis in the lack of close significant phylogenetic relationship between meadow species that have similar \^{I}$\pm$ niches. In a second test, \^{I}$\pm$ niche overlap based on a variety of traits is compared between congeners and noncongeners in several communities; here, too, there is no evidence of a correlation between \^{I}$\pm$ niche and phylogeny. To test whether \^{I}² and \^{I}³ niches evolve conservatively, we reconstructed the evolution of relevant traits on evolutionary trees for 14 different clades. Tests against null models revealed a number of instances, including some in island radiations, in which habitat (\^{I}² niche) and elevational maximum (an aspect of the \^{I}³ niche) showed evolutionary conservatism.},
	Annote = {ArticleType: primary\_article / Issue Title: Supplement: Phylogenetic Approaches to Community Ecology / Full publication date: Jul., 2006 / Copyright \^{A}{\copyright} 2006 Ecological Society of America},
	Author = {Silvertown, Jonathan and Dodd, Mike and Gowing, David and Lawson, Clare and McConway, Kevin},
