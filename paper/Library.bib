	Journal = {Science},
	Number = {5800},
	Pages = {787--790},
	Title = {{Impacts of Biodiversity Loss on Ocean Ecosystem Services}},
	Volume = {314},
	Year = {2006},
	Bdsk-Url-1 = {http://www.sciencemag.org/cgi/content/abstract/314/5800/787}}

