	Issn = {1537-5323},
	Journal = {The American Naturalist},
	Keywords = {Adaptation, Biological,Adaptation, Biological: physiology,Computer Simulation,Desert Climate,Ecosystem,Extinction, Biological,Mexico,Models, Biological,Plants,Plants: growth \& development,Species Specificity,community phylogenetics,facilitation},
	Mendeley-Tags = {community phylogenetics,facilitation},
